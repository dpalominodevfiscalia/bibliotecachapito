@model BibliotecaDB.Models.MovimientoAlmacen

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.TipoMovimiento.Nombre)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.TipoMovimiento.Nombre)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.AlmacenOrigen.Nombre)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.AlmacenOrigen.Nombre)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.AlmacenDestino.Nombre)
    </dt>
    <dd class="col-sm-10">
        @(Model.AlmacenDestino != null ? Model.AlmacenDestino.Nombre : "-")
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Producto.Nombre)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Producto.Nombre)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Cantidad)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Cantidad)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Observaciones)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Observaciones)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.DocumentoReferencia)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.DocumentoReferencia)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.FechaMovimiento)
    </dt>
    <dd class="col-sm-10">
        @Model.FechaMovimiento.ToString("dd/MM/yyyy HH:mm")
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Usuario)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Usuario)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Estado)
    </dt>
    <dd class="col-sm-10">
        @(Model.Estado ? "Activo" : "Inactivo")
    </dd>
</dl>

<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
</div>